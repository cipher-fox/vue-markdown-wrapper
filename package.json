    "marked": "^15.0.12"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/dompurify": "^3.2.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.4",
    "typescript": "^5.5.4",
    "vite": "^6.3.5",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.16",
    "vue-tsc": "^2.2.10"
  }
